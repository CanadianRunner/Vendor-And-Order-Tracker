@using VendorOrder.Models;

<h1>Here's a list of our vendors!</h1>
@if (@Model.Count == 0)
{
  <h3>There's nothing in this list.</h3>
}

@foreach (Vendor customer in Model)
{
  <h3><a class="btn" href="/vendors/@customer.Id">@customer.Name</a></h3>
  
}

<a href="/" class="btn">Take me home country roads</a>
<a href="/vendors/new" class="btn">Add new Vendor</a>